generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model word {
  word           String? @db.VarChar(100)
  unit_type      String? @db.VarChar(100)
  type           String? @db.VarChar(100)
  origin_word    String? @db.VarChar(100)
  part_of_speech String? @db.VarChar(100)
  definition     String? @db.Text
  examples       String? @db.Text
  subject        String? @db.VarChar(100)
  proverb        String? @db.Text
  idiom          String? @db.Text
  related_words  String? @db.Text
  id             Int     @id @default(autoincrement())
  len            Int?    @db.TinyInt

  @@index([len], map: "idx_len")
  @@index([word], map: "idx_word")
  @@index([len, unit_type], map: "idx_len_unit")
}

model fiveinitial {
  ch  String  @id @db.VarChar(10)
  ch1 String? @db.VarChar(20)
  ch2 String? @db.VarChar(20)
  ch3 String? @db.VarChar(20)
  ch4 String? @db.VarChar(20)
  ch5 String? @db.VarChar(20)
  ch6 String? @db.VarChar(20)
}

